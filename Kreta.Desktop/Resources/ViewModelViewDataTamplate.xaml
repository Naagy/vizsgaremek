<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controlPanelViewModels="clr-namespace:Real.Desktop.ViewModels.ControlPanel"
                    xmlns:controlPanelView="clr-namespace:Real.Desktop.Views.ControlPanel"
                    xmlns:realCitizensViewModels="clr-namespace:Real.Desktop.ViewModels.RealCitizens"
                    xmlns:realCitizensView="clr-namespace:Real.Desktop.Views.RealCitizens"
                    >
    <DataTemplate DataType="{x:Type controlPanelViewModels:ControlPanelViewModel}">
        <controlPanelView:ControlPanelView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type realCitizensViewModels:RealCitizensViewModel}">
        <realCitizensView:RealCitizensView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type realCitizensViewModels:PlayerViewModel}">
        <realCitizensView:PlayerView/>
    </DataTemplate>
</ResourceDictionary>